    "eslint-config-concise-esnext": "^0.34.0",
    "eslint-config-concise-jest": "^0.34.0",
    "esm": "^3.0.84",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier-eslint-cli": "^4.7.1"
  },
  "repository": "ratson/react-native-firebase-test-lab",
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "files": [
    "*.md",
    "index.js",
    "index.android.js",
    "android"
  ],
  "keywords": [
    "react-native"
  ],
  "lint-staged": {
